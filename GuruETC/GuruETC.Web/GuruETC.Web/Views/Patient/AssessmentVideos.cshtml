<script type="text/javascript">
    $(document).ready(function () {
        $(".gallery:first a[rel^='prettyPhoto']").prettyPhoto({ animation_speed: 'normal', theme: 'facebook', slideshow: 3000, social_tools: false, autoplay_slideshow: false });
        $(".gallery:gt(0) a[rel^='prettyPhoto']").prettyPhoto({ animation_speed: 'fast', slideshow: 10000, social_tools: false, hideflash: true });
    });
</script>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
